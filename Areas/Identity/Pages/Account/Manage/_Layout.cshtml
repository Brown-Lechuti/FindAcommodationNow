@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}
<div style="background-color:white;overflow-y:auto;height:500px">
    <h2>Res Bookings</h2>
    <div>
        <h4>View & apply to your favourite res</h4>
        <hr />
        <div class="row" ;>
            <div class="col-md-3">
                <partial name="_ManageNav" />
            </div>
            <div class="col-md-9">
                @RenderBody()
            </div>
        </div>
    </div>
    @section Scripts {
        @RenderSection("Scripts", required: false)
    }
</div>
